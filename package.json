{
  "name": "prisma-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "prebuild:android": "expo prebuild -p android",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^6.1.7",
    "@shopify/flash-list": "1.6.4",
    "@tamagui/font-inter": "1.74.8",
    "@tamagui/react-native-media-driver": "1.74.8",
    "@tamagui/shorthands": "1.74.8",
    "@tamagui/themes": "1.74.8",
    "@tanstack/react-query": "^5.59.20",
    "axios": "^1.7.7",
    "expo": "51",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.28",
    "expo-font": "~12.0.10",
    "expo-image": "~1.13.0",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "pinar": "^0.12.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-quick-base64": "^2.1.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.8.0",
    "react-native-web": "~0.19.10",
    "sonner-native": "^0.16.0",
    "tamagui": "1.74.8",
    "zustand": "^5.0.1"
  },
  "overrides": {
    "@expo/prebuild-config": "~6.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tamagui/babel-plugin": "1.74.8",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
}
